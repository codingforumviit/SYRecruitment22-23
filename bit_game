#include <bits/stdc++.h>
#define ll long long int
#define ld long double
using namespace std;
 
void mainSolve()
{
  int n;
  cin >> n;
  for (int i = 1; i <= n; i++)
    cout << (2 * i + 1) << ' ';
  cout << endl;
  for (int i = 1; i <= n; i++)
    cout << (2 * i) << ' ';
  cout << endl;
}
 
int main()
{
  int t;
  cin >> t;
  while (t--)
  {
    mainSolve();
  }
  return 0;
}
